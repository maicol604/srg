{
  "expo": {
    "name": "Grandes Vinos Galicia",
    "slug": "vivasrg-mstore",
    "version": "5.0.4",
    "orientation": "portrait",
    "icon": "./assets/icon-srg.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#212121"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.app.vivasrgstore",
      "config": {
        "usesNonExemptEncryption": false
      },
      "buildNumber": "1.0.4",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-srg.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.app.vivasrgstore",
      "versionCode": 5
    },
    "extra": {
      "eas": {
        "projectId": "12b112b2-163e-408c-87f5-a2947c43f2ac"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
